projection;
strict ( 2 );
use draft;

define behavior for ZC_CLASS_01 alias Class
use etag
{
  use create;
  use update;
  use delete;

  use association _Students { create; with draft; }
  use association _Teacher { with draft; }

  use action assignTeacher;
  use action removeTeacher;
}

define behavior for ZC_STUDENT_01 alias Student
use etag
{
  use update;
  use delete;

  use association _Class { with draft; }
} 